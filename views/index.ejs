<script>

var enterUser;

/*
 * Compatibility Testing
 */

// wrapper function to remove element from IE
function removeElement(element) {
    element && element.parentNode && element.parentNode.removeChild(element);
}

// Check for IE
(function(){
    var N= navigator.appName, ua= navigator.userAgent, tem;
    var M= ua.match(/(opera|chrome|safari|firefox|msie)\/?\s*(\.?\d+(\.\d+)*)/i);
    if(M && (tem= ua.match(/version\/([\.\d]+)/i))!= null) M[2]= tem[1];
    //M= M? [M[1], M[2]]: [N, navigator.appVersion,'-?'];

    if(M[1] == "MSIE"){
      alert("Sorry! Internet Explorer is currently unsupported by Studionet. Please switch to a different browser.");
      removeElement( document.getElementById('form_user') );
      removeElement( document.getElementById('login') );
    }
    else{
      enterUser = function(){
          document.getElementById("form_user").submit();
      }
    }

})();




</script>